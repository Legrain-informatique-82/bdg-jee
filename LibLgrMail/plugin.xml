<plugin>
   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            id="LibLgrMail.actionSet"
            label="Mail Action Set"
            visible="true">
         <menu
               id="mailMenu"
               label="Mail">
            <separator
                  name="mailGroup">
            </separator>
         </menu>
         <action
               class="fr.legrain.libLgrMail.actions.MailAction"
               icon="icons/script.png"
               id="liblgrmail.actions.MailAction"
               label="Fichiers de log"
               menubarPath="--help/mailGroup --envoi de mail pas au point"
               tooltip="Mail">
         </action>
      </actionSet>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="fr.legrain.libLgrMail.preferences.PreferenceInitializer">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="fr.legrain.libLgrMail.preferences.LgrMailPreferencePage"
            id="LibLgrMail.page1"
            name="E-Mail">
      </page>
   </extension>
</plugin>
