<plugin>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="fr.legrain.avisecheance.editor.AvisEcheanceMultiPageEditor"
            icon="icons/creditcards.png"
            id="fr.legrain.editor.avisEcheance.swt.multi"
            name="Avis d&apos;échéance">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="fr.legrain.avisecheance.handlers.HandlerOuvreEditorAvisEcheance"
            description="Nouvel avis d&apos;échéance"
            id="fr.legrain.editor.avisEcheance.swt.multi"
            name="Nouvel avis d&apos;échéance">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:documents?after=separator.facture">
         <command
               commandId="fr.legrain.editor.avisEcheance.swt.multi"
               icon="icons/creditcards.png"
               id="fr.legrain.editor.avisEcheance.swt.multi"
               label="Nouvel avis d&apos;échéance">
         </command>
      </menuContribution>
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.main.toolbar">
         <toolbar
               id="toolbar.avisEcheance">
         </toolbar>
      </menuContribution>
      <menuContribution
            locationURI="toolbar:toolbar">
         <command
               commandId="fr.legrain.editor.avisEcheance.swt.multi"
               icon="icons/creditcards.png"
               label="Nouvel avis d&apos;échéance">
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="fr.legrain.avisecheance.preferences.PreferenceInitializer">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="fr.legrain.document.preferences.DocumentPreferencePage"
            class="fr.legrain.avisecheance.preferences.LgrAvisEcheancePreferencePage"
            id="fr.legrain.avisecheance.preferences.LgrAvisEcheancePreferencePage"
            name="avis d&apos;échéance">
      </page>
   </extension>
   <extension
         point="org.eclipse.ui.intro.configExtension">
      <configExtension
            configId="GestionCommerciale.introConfigId"
            content="intro/introContent.xml">
      </configExtension>
   </extension>
   <extension
         point="GestionCommerciale.ImpressionDocument">
      <impressionDocument
            ClassImpressionDocument="fr.legrain.avisecheance.divers.Impression"
            TypeDoc="avisEcheance">
      </impressionDocument>
   </extension>
   <extension
         point="GestionCommerciale.Preferences">
      <Preferences
            ClassPreferences="fr.legrain.avisecheance.preferences.PreferencesStore">
      </Preferences>
   </extension>

</plugin>
