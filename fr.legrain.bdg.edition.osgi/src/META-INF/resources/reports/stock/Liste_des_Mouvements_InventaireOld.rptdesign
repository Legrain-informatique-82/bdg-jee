            <method name="open"><![CDATA[count=0;

Inventaire = reportContext.getHttpServletRequest().getSession().getAttribute("inventaire");
liste = new Packages.java.util.LinkedList();
liste = reportContext.getHttpServletRequest().getSession().getAttribute("listeInventaire");

Packages.java.util.logging.Logger.getLogger("").info("Inventaire - "+Inventaire);
 

Packages.java.util.logging.Logger.getLogger("").info("liste - "+liste);]]></method>
            <method name="fetch"><![CDATA[libDate = new Packages.fr.legrain.lib.data.LibDate();
if(count < liste.size()) {
	
	Inventaire = liste.get(count);
	
	if(Inventaire.getLibelleInventaire()==null){row["libelleInventaire_ligne"] = "";}
	else{row["libelleInventaire_ligne"]=Inventaire.getLibelleInventaire();}
	//
	if(Inventaire.getTaLot()==null 
		|| Inventaire.getTaLot().getTaArticle()==null 
		|| Inventaire.getTaLot().getTaArticle().getTaFamille()==null 
		|| Inventaire.getTaLot().getTaArticle().getTaFamille().getLibcFamille()==null){row["familleInventaire_ligne"] = "";}
	else{row["familleInventaire_ligne"]=Inventaire.getTaLot().getTaArticle().getTaFamille().getLibcFamille();}
	//
	if(Inventaire.getQteReelle()==null){row["qteReelle"] = "";}
	else{row["qteReelle"]=Inventaire.getQteReelle();}
	//
	if(Inventaire.getUn1Inventaire()==null){row["un1Inventaire"] = "";}
	else{row["un1Inventaire"]=Inventaire.getUn1Inventaire();}
	//
	if(Inventaire.getQteTheorique()==null){row["qteTheorique"] = "";}
	else{row["qteTheorique"]=Inventaire.getQteTheorique();}
	//
	if(Inventaire.getQte2LInventaire()==null){row["qte2LInventaire"] = "";}
	else{row["qte2LInventaire"]=Inventaire.getQte2LInventaire();}
	//
	if(Inventaire.getUn2Inventaire()==null){row["un2Inventaire"] = "";}
	else{row["un2Inventaire"]=Inventaire.getUn2Inventaire();}
	//
	if(Inventaire.getTaLot().getNumLot()==null){row["numLot"] = "";}
	else{row["numLot"]=Inventaire.getTaLot().getNumLot();}
	//
	if(Inventaire.getQteReelle()==null &amp;&amp; Inventaire.getQteTheorique()==null){row["qteEcart"] = "";}
	else{row["qteEcart"]=(Inventaire.getQteTheorique() - Inventaire.getQteReelle());}
	//
	row["quantiteR"] = (row["qteReelle"]+" "+row["un1Inventaire"]);
	row["quantiteT"] = (row["qteTheorique"]+" "+row["un1Inventaire"]);
	row["quantiteE"] = (row["qteEcart"]+" "+row["un1Inventaire"]);

	if(Inventaire.getQuandCree()==null){row["dateCree"] = "";}
	else{
		d = new Date();
		Packages.java.util.logging.Logger.getLogger("").info("NEW date - "+d);
		d = libDate.dateToString(Inventaire.getQuandCree());
		Packages.java.util.logging.Logger.getLogger("").info("dateCree - "+d);
		row["dateCree"]=d;
		Packages.java.util.logging.Logger.getLogger("").info("row[\"dateCree\"] - "+row["dateCree"]);
	}
	//
	row["Entrepot"]="";
	if(Inventaire.getTaEntrepot()!=null){
	if(Inventaire.getTaEntrepot().getLibelle()==null){row["Entrepot"] = "";}
	else{row["Entrepot"]=Inventaire.getTaEntrepot().getLibelle();}
	}
	//
	row["matierePremiere"]="";
	if(Inventaire.getTaLot()!=null){
	if(Inventaire.getTaLot().getTaArticle().getMatierePremiere()==true){row["matierePremiere"] = "MP";}
	else{row["matierePremiere"] = "--";}
	}
	//
	row["codeArticle"]="";
	row["produitFini"]="";
	if(Inventaire.getTaLot().getTaArticle()!=null){
	//
	if(Inventaire.getTaLot().getTaArticle().getCodeArticle()==null){row["codeArticle"] = "";}
	else{row["codeArticle"]=Inventaire.getTaLot().getTaArticle().getCodeArticle();}
	//	
	if(Inventaire.getTaLot().getTaArticle().getProduitFini()==true){row["produitFini"] = "PF";}
	else{row["produitFini"] = "--";}
	}
	//
	row["PFMP"] = row["matierePremiere"] +" / "+ row["produitFini"];
	
	count++;
	return true;
}

	


return false;
]]></method>
