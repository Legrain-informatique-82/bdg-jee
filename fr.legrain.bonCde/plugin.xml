<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="fr.legrain.boncde.editor.BoncdeMultiPageEditor"
            icon="icons/calculator_edit.png"
            id="fr.legrain.editor.boncde.swt.multi"
            name="Bon de commande">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="fr.legrain.boncde.handlers.HandlerOuvreEditorBoncde"
            description="Nouvelle commande"
            id="fr.legrain.boncde"
            name="Nouvelle commande">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:documents?after=separator.facture">
         <command
               commandId="fr.legrain.boncde"
               icon="icons/calculator_edit.png"
               id="fr.legrain.boncde"
               label="Commande">
         </command>
      </menuContribution>
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.main.toolbar">
         <toolbar
               id="toolbar.boncde">
         </toolbar>
      </menuContribution>
      <menuContribution
            locationURI="toolbar:toolbar">
         <command
               commandId="fr.legrain.boncde"
               icon="icons/calculator_edit.png"
               label="Commande">
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="fr.legrain.boncde.preferences.PreferenceInitializer">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="fr.legrain.document.preferences.DocumentPreferencePage"
            class="fr.legrain.boncde.preferences.LgrBoncdePreferencePage"
            id="fr.legrain.boncde.preferences.LgrBoncdePreferencePage"
            name="Bon de commande">
      </page>
   </extension>
   <extension
         point="org.eclipse.ui.intro.configExtension">
      <configExtension
            configId="GestionCommerciale.introConfigId"
            content="intro/introContent.xml">
      </configExtension>
   </extension>
   <extension
         point="GestionCommerciale.ImpressionDocument">
      <impressionDocument
            ClassImpressionDocument="fr.legrain.boncde.divers.Impression"
            TypeDoc="Boncde">
      </impressionDocument>
   </extension>
   <extension
         point="GestionCommerciale.Preferences">
      <Preferences
            ClassPreferences="fr.legrain.boncde.preferences.PreferencesStore">
      </Preferences>
   </extension>

</plugin>
