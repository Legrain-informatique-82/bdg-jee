<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="fr.legrain.publipostage.gui.preferences.PreferenceInitializer">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="fr.legrain.publipostage.gui.base"
            class="fr.legrain.publipostage.gui.preferences.PublipostageModelePreferencePage"
            id="fr.legrain.publipostage.gui.modele"
            name="Modele publipostage">
      </page>
      <page
            class="fr.legrain.publipostage.gui.preferences.PublipostageBasePreferencePage"
            id="fr.legrain.publipostage.gui.base"
            name="Publipostage">
      </page>
   </extension>

</plugin>
