//	}
//	list = imprimeObjet.l;
jpql = "select tlf from TaLFacture tlf " +
	   "where tlf.taDocument.dateDocument " +
	   "between " +
	   "(select tie.datedebInfoEntreprise from TaInfoEntreprise tie) " +
	   "and " +
	   "(select tie.datefinInfoEntreprise from TaInfoEntreprise tie) "+
	   "and tlf.taArticle.codeArticle is not null "+
	   "order by tlf.taDocument.dateDocument";

entityManager = null;

imprimeObjet = new Packages.fr.legrain.edition.ImprimeObjet();
entityManager = imprimeObjet.listEntityManager.get(0); 
	   
//a = new Packages.fr.legrain.lib.data.JPABdLgr();	     			  
//query = a.getEntityManager().createQuery(jpql);
query = entityManager.createQuery(jpql);

imprimeObjet.l.clear();
for (i = 0; i < query.getResultList().size(); i++) {
	lineFacture = new Packages.fr.legrain.documents.dao.TaLFacture();
	lineFacture = query.getResultList().get(i);
	//lineFacture.calculMontant();
	imprimeObjet.l.add(lineFacture);
}
list = imprimeObjet.l;]]></method>
            <method name="fetch"><![CDATA[if(count< list.size()) {
	object = list.get(count);
	
	row["codeArticle"] = object.getTaArticle().getCodeArticle();
	//row["mnHT"] = object[1].floatValue();
	//row["mnTTC"] = object[2].floatValue();
	//row["mnTVA"] = row["mnTTC"]-row["mnHT"];
	
	if(row["mnHT"] = object.getMtHtLApresRemiseGlobaleDocument() == null){
		row["mnHT"] = 0.0;
	}else{
		row["mnHT"] = object.getMtHtLApresRemiseGlobaleDocument().floatValue();
	}
	
	if(row["mnTTC"] = object.getMtTtcLApresRemiseGlobaleDocument() == null){
		row["mnTTC"] = 0.0;
	}else{
		row["mnTTC"] = object.getMtTtcLApresRemiseGlobaleDocument().floatValue();
	}
	row["mnTVA"] = row["mnTTC"] - row["mnHT"];
	count++;
	return true;
}
return false;]]></method>
